{
  "expo": {
    "name": "Kloc",
    "slug": "kloc",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kloc",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.kesmek.kloc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#111113"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.kesmek.kloc"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#111113",
          "width": 200,
          "image": "./assets/images/icon.png",
          "resizeMode": "contain"
        }
      ],
      "expo-sqlite",
      "react-native-edge-to-edge",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "13c4b178-83ca-49f2-8aba-4f6198fe7ded"
      }
    },
    "owner": "kesmek"
  }
}
